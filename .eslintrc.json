{
    "extends": ["airbnb-base"],
        "env":{
          "node":true,
          "es6":true,
          "commonjs":true
        },
    "rules": {
        "semi": [0, "always"],
        "linebreak-style": [0, "windows"],
        "no-unused-vars":1,
        "import/order":0,
        "no-use-before-define":0,
        "no-restricted-globals":0,
        "no-shadow":0,
        "no-console":0
    }
}